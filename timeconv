# Python program to convert time
# from 12 hour to 24 hour format
def convert24(str1):
    if str1[-2:]=="AM" and str1[:2]=="12":
        return "00"+str1[2:-2]
    elif str1[-2:]=="AM":
        return str1[:-2]
    elif str1[-2:]=="PM" and str1[:2]=="12":
        return str1[:-2]
    else:
        return str(int(str1[:2])+12)+str1[2:8]
print(convert24("11:05:45 PM"))
print(convert24("05:10:40 AM"))
print(convert24("05:05:20 PM"))
print(convert24("09:20:11 AM"))
print(convert24("10:10:10 PM"))
